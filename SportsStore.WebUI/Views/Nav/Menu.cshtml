@model IEnumerable<string>
@Html.ActionLink("All", "List", "Product")

@foreach (string link in Model)
{
    @Html.RouteLink(link,
        new
        {
            controller = "Product",
            action = "List",
            category = link,
            page = 1
        },
        new
        {
            @class = link == ViewBag.SelectedCategory ? "selected" : null
        }
    );
}
